{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["eslint:recommended", "airbnb"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "consistent-return": "warn",
        "no-underscore-dangle": ["warn", { "allow": ["_id"] }],
        "no-unused-vars": ["warn"]
    }
  }
  